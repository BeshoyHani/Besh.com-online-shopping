<!-- Script to Siaplay Message when item is added to Cart -->
<script>
    let toastMessage = `<%=toastMessage%>`
    const jsFrame = new JSFrame();
    if(toastMessage){
    jsFrame.showToast({
        html: toastMessage, align: 'top', duration: 2000
    });
}
</script>

